	<queryString language="SQL">
		<![CDATA[SELECT mc.codigo as c1, mc.nombre as c2, mq.codigo AS c3, mq.nombre AS c4, COALESCE(mq.existencia, 0) AS c5
FROM marca_equipo AS mc
LEFT JOIN modelo_equipo AS mq ON mc.id = mq.id_marca
