	<queryString>
		<![CDATA[SELECT mc.codigo as c1, mc.nombre as c2, mq.codigo as c3, mq.nombre as c4, eq.nombre as c5, eq.serie as c6, eq.inventario as c7, eq.fecha_adquisicion as c8, df.fecha_ingreso as c9
